generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider     = "mysql"
    url          = env("DATABASE_URL")
    relationMode = "prisma"
}

model User {
    discordId String  @id
    watchlist Watch[]
}

model Watch {
    address String @id
    symbol  String
    below   Float

    User          User?   @relation(fields: [userDiscordId], references: [discordId])
    userDiscordId String?
}
